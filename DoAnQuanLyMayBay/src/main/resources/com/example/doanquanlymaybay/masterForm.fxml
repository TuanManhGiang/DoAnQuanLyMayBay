<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="2000.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.doanquanlymaybay.controler.MasterFormController">
   <children>
      <BorderPane fx:id="mainPane" prefWidth="2000.0">
         <left>
            <VBox prefHeight="509.0" prefWidth="125.0" spacing="20.0" BorderPane.alignment="CENTER">
                <padding>
                    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                </padding>
               <Button mnemonicParsing="false" onAction="#handleButtonPlaneAction" prefHeight="25.0" prefWidth="106.0" text="Plane" />
               <Button mnemonicParsing="false" onAction="#handleButtonFlightAction" prefHeight="25.0" prefWidth="99.0" text="Flight" />
                <Button onAction="#handleButtonTicketAction" prefHeight="27.0" prefWidth="100.0" text="Ticket" />
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </VBox>
         </left>
      </BorderPane>
   </children>
</AnchorPane>
